{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "schematics": {
    "@schematics/angular:component": {
      "style": "scss",
      "skipTests": true
    },
    "@schematics/angular:guard": {
      "skipTests": true
    },
    "@schematics/angular:pipe": {
      "skipTests": true
    },
    "@schematics/angular:service": {
      "skipTests": true
    }
  },
  "projects": {
    "control-panel": {
      "projectType": "application",
      "root": "projects/control-panel",
      "sourceRoot": "projects/control-panel/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/chan-da-liar",
            "index": "projects/control-panel/src/index.html",
            "main": "projects/control-panel/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/control-panel/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/control-panel/src/favicon.ico",
              "projects/control-panel/src/assets"
            ],
            "styles": [
              "projects/control-panel/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "control-panel:build:production"
            },
            "development": {
              "buildTarget": "control-panel:build:development"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    },
    "mobile": {
      "projectType": "application",
      "root": "projects/mobile",
      "sourceRoot": "projects/mobile/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/mobile",
            "index": "projects/mobile/src/index.html",
            "browser": "projects/mobile/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/mobile/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/mobile/src/favicon.ico",
              "projects/mobile/src/assets"
            ],
            "styles": [
              "projects/mobile/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/mobile/src/environments/environment.ts",
                  "with": "projects/mobile/src/environments/environment.production.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "mobile:build:production"
            },
            "development": {
              "buildTarget": "mobile:build:development"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
